{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    ".github": true,
    "lib": true,
    "bin": true,
    ".vscode": true,
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.md": "$(capture)_*.md",
    "package.json": ".*, README.md, *.md, *.json, *.yaml, *.cjs, *.lock, LICENSE"
  }
}
